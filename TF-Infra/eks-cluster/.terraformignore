*.swp
*.tmp
*.bak
*.DS_Store
Thumbs.db

# Ignore local override files
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# Ignore Terraform crash logs
crash.log

# Ignore sensitive files
secrets.tfvars
*.pem
*.key

# Ignore Terraform state backups (if needed)
*.tfstate.backup

# Ignore test and temp directories
test/
tests/
tmp/
temp/

# Ignore node and Python environments
node_modules/
venv/
.env/
